<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>1</servlet-name>
		<servlet-class>com.demo.controller.register_servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>2</servlet-name>
		<servlet-class>com.demo.controller.login_servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>3</servlet-name>
		<servlet-class>com.demo.controller.logout_servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>4</servlet-name>
		<servlet-class>com.demo.controller.addBook_servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>9</servlet-name>
		<servlet-class>com.demo.controller.delete_servlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>5</servlet-name>
		<servlet-class>com.demo.controller.update_servlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>1</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>2</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>3</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>4</servlet-name>
		<url-pattern>/addBook</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>5</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>9</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
</web-app>
